{
  "version": 2,
  "builds": [
    {
      "src": "oceanbackend/api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "OceanFrontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/oceanbackend/api/index.ts"
    },
    {
      "source": "/uploads/:path*",
      "destination": "/oceanbackend/uploads/:path*"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
